{
  "name": "d-una-delivery-api",
  "version": "1.0.0",
  "description": "API for Duna delivery app and generator of random data using recent timestamps for testing the application during development phase",
  "engines": {
    "node": "12.18.1"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc -b tsconfig.json src/tsconfig.json seed/tsconfig.json",
    "start": "node built/src/index.js",
    "seed": "npm run build && node built/seed/index.js",
    "seed:dev": "nodemon --watch seed seed/index.ts"
  },
  "main": "built/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/normancarcamo/d-una-delivery-api.git"
  },
  "keywords": [
    "d-una", "delivery"
  ],
  "author": {
    "name": "Norman Carcamo",
    "email": "normancarcamo@gmail.com"
  },
  "license": "SEE LICENSE IN LICENSE FILE",
  "bugs": {
    "url": "https://github.com/normancarcamo/d-una-delivery-api/issues"
  },
  "maintainers": [
    "Norman Carcamo <normancarcamo@gmail.com> (normancarcamo.com)"
  ],
  "homepage": "https://github.com/normancarcamo/d-una-delivery-api#readme",
  "contributors": [
    {
      "name": "Norman Carcamo",
      "email": "normancarcamo@gmail.com"
    }
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bson-objectid": "^1.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.23.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.20",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/helmet": "0.0.47",
    "@types/lodash": "^4.14.157",
    "@types/mongodb": "^3.5.23",
    "@types/mongoose": "^5.7.27",
    "@types/node": "^14.0.23",
    "jest": "^26.1.0",
