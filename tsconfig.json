{
  "compilerOptions": {
    "outDir": "./built",
    "baseUrl": "src",
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "**/*.test.ts", "built"],
  "include": ["**/*.ts"]
}
